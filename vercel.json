{
    "version": 2,
    "buildCommand": "cd invoice-react && npm install && npm run build",
    "outputDirectory": "invoice-react/dist",
    "rewrites": [
        {
            "source": "/api/test",
            "destination": "/api/test.js"
        },
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        },
        {
            "source": "/auth/:path*",
            "destination": "/api/index.js"
        },
        {
            "source": "/assets/:path*",
            "destination": "/assets/:path*"
        },
        {
            "source": "/((?!api/.*|auth/.*|assets/.*).*)",
            "destination": "/index.html"
        }
    ]
}